dat = runif(20, 0, 1)
df <- data.frame(
x = c(dat, dat),
g = gl(2, 100)
)
fig = ggplot(df, aes(x)) +
stat_ecdf(geom = 'step') +
xlab('Time to maturity') +
ylab('Value paid incl fees') +
ggtitle('Uniform cumulative payout') +
theme_bw() +
theme(text = element_text(size=14, family='Times New Roman'))
